            ja:externalContent <file:./imports/ontologies/w3c/org.ttl> ;
            ja:externalContent <file:./imports/ontologies/w3c/owl.ttl> ;
            ja:externalContent <file:./imports/ontologies/w3c/prov.ttl> ;
